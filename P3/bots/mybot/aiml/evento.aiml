<category>
  <pattern>^ eliminar ^</pattern>
  <that>^ DESEA AÑADIR O ELIMINAR UN CONTACTO DEL EVENTO</that>
  <template>Dime el apellido del contacto que quieres eliminar</template>
</category>

<category>
  <pattern>*</pattern>
  <that>DIME EL APELLIDO DEL CONTACTO QUE QUIERES ELIMINAR</that>
  <template>
    <think><set var="pos_contacto"><srai>FINDITEM <srai>ponerbarrabaja <star/></srai> IN <map name="evper"><get name="fecha_evento"/>_<srai>QUITARDOSPUNTOS <get name="hora_evento"/></srai></map> º</srai></set></think>
    <condition var="pos_contacto">
      <li value="0">El contacto que intenta eliminar no se encuentra registrado en el evento, desea añadir o eliminar un contacto del evento?
      </li>
      <li>
        <modifymap>
          <name>evper</name>
          <key><get name="fecha_evento"/>_<srai>QUITARDOSPUNTOS <get name="hora_evento"/></srai></key>
          <value><srai>REMOVEITEM <get var="pos_contacto"/> IN <map name="evper"><get name="fecha_evento"/>_<srai>QUITARDOSPUNTOS <get name="hora_evento"/></srai></map></srai></value>
        </modifymap>
        <think><set var="list_contactos"><map name="evper"><get name="fecha_evento"/>_<srai>QUITARDOSPUNTOS <get name="hora_evento"/></srai></map></set></think>
        <condition var="list_contactos">
          <li value="$">
            <removefrommap>
              <name>evper</name>
