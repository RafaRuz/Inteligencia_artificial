        </think>
        <condition var="palabra">
          <li value="null">
            <think>
              <set var="palabra"><srai>TOP <get var="eventos"/></srai></set>
              <set var="eventos"><srai>REMAIN <get var="eventos"/></srai></set>
            </think>
            <loop/>
